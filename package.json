{
  "name": "wt-nodejs-api",
  "version": "0.1.1",
  "description": " API to interact with the Winding Tree platform",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src test",
    "test": "npm run lint; ETH_NETWORK=test ./test/utils/scripts/localtestnet.sh",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "now-start": "ETH_NETWORK=testnet node ./src/index.js",
    "dev": "ETH_NETWORK=local node ./src/index.js",
    "deploy": "now -e NODE_ENV=production --token $NOW_TOKEN --npm --public",
    "alias": "now alias --token=$NOW_TOKEN",
    "local-net": "node_modules/.bin/ganache-cli -i 77 --gasLimit 6000000",
    "local-deploy": "node ./test/deployer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windingtree/wt-nodejs-api.git"
  },
  "keywords": [
    "winding-tree",
    "api",
    "nodejs",
    "javascript"
  ],
  "author": "WT Dev Team <support@winidngtree.com>",
  "license": "APACHE 2.0",
  "bugs": {
    "url": "https://github.com/windingtree/wt-nodejs-api/issues"
  },
  "homepage": "https://github.com/windingtree/wt-nodejs-api#readme",
  "dependencies": {
    "@windingtree/wt-js-libs": "^0.2.1",
    "body-parser": "^1.18.2",
    "compose-middleware": "^4.0.0",
    "express": "^4.16.3",
    "express-rate-limit": "^2.11.0",
    "lodash": "^4.17.10",
    "swagger-ui-express": "^3.0.8",
    "web3": "^1.0.0-beta.34"
  },
  "devDependencies": {
    "@windingtree/wt-contracts": "^0.2.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "ganache-cli": "^6.1.0",
    "mocha": "^5.1.0",
    "nyc": "^11.6.0",
    "sinon": "^4.5.0",
    "supertest": "^3.0.0",
    "truffle-contract": "^3.0.5"
  },
  "engines": {
    "node": "8.9.4"
  },
  "now": {
    "name": "wt-nodejs-api",
    "alias": "demo-api.windingtree.com"
  }
}
